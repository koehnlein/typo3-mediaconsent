{
    "name": "arbkomekvw/mediaconsent",
    "type": "typo3-cms-extension",
    "description": "Extension loads content only if the user agrees.",
    "homepage": "https://github.com/abkekvw/mediaconsent",
    "keywords": [
        "TYPO3 CMS",
        "ekvw",
        "DSGVO",
        "GDPR",
        "front-end"
    ],
    "license": ["GPL-2.0-or-later"],
    "authors": [
        {
            "name": "Christoph Roth",
            "email": "christoph.roth@lka.ekvw.de",
            "role": "Developer",
            "homepage": "http://www.ekvw.de/kommunikation/"
        }
    ],
    "require": {
        "php": ">=7.0",
        "typo3/cms-core": ">=9.5.0  <=10.4.99"
    },
    "autoload": {
        "psr-4": {
            "ArbkomEKvW\\mediaconsent\\": "Classes/"
        }
    },
    "replace": {
        "arbkomekvw/mediaconsent": "self.version",
        "typo3-ter/mediaconsent": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "mediaconsent"
        }
    }
}
